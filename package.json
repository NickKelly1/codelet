{
  "name": "codelets",
  "type": "module",
  "main": "dist/main.js",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "Self hosted code snippets",
  "author": "Nick Kelly",
  "homepage": "https://github.com/NickKelly1/codelets#readme",
  "readme": "https://github.com/NickKelly1/codelets#readme",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/NickKelly1/codelets.git"
  },
  "bugs": {
    "url": "https://github.com/NickKelly1/codelets/issues"
  },
  "keywords": [
    "TypeScript",
    "JavaScript"
  ],
  "scripts": {
    "start": "node --enable-source-maps dist/main.js",
    "start:inspect": "node --inspect --enable-source-maps dist/main.js",
    "start:inspect-brk": "node --inspect-brk --enable-source-maps dist/main.js",
    "dev": "pnpm clean && nodemon --ext hbs,ts,js,jsx,tsx --delay 0.10 --watch src --watch views --exec 'pnpm build && pnpm start'",
    "dev:inspect": "pnpm clean && nodemon --ext hbs,ts,js,jsx,tsx --delay 0.10 --watch src --watch views --exec 'pnpm build && pnpm start:inspect'",
    "dev:inspect-brk": "pnpm clean && nodemon --ext hbs,ts,js,jsx,tsx --delay 0.10 --watch src --watch views --exec 'pnpm build && pnpm start:inspect-brk'",
    "build": "tsc -p config/tsconfig.build.json",
    "build:watch": "tsc -p config/tsconfig.build.json --watch",
    "build:clean": "pnpm run clean && pnpm run build",
    "build:clean:watch": "pnpm run clean && pnpm run build:watch",
    "clean": "rimraf dist .tsbuildinfo",
    "test": "jest -c config/jest.config.ts",
    "test:coverage": "jest -c config/jest.config.ts --coverage",
    "test:watch": "jest -c config/jest.config.ts --watch",
    "lint": "eslint . --ext '*/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint . --ext '*/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.5",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/hbs": "^4.0.1",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.0",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/platform-socket.io": "^8.2.5",
    "@nestjs/websockets": "^8.2.5",
    "@nkp/age": "^0.0.2",
    "@nkp/array-map": "^0.0.7",
    "@nkp/config": "0.0.18",
    "@nkp/delay": "^0.0.1",
    "@nkp/kv": "^0.0.4",
    "@nkp/linked-list": "^0.0.5",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.2",
    "dotenv": "^14.1.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "mkdirp": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.5.5"
  }
}
